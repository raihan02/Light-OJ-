#include <bits/stdc++.h>
#define sc scanf
#define pf printf
#define nl printf("\n")
typedef long long int ll;
using namespace std;

int main()
{
  string s;
  ll tes, n , o = 0;
  sc("%lld", &tes);
  while(tes--)
  {
      o++;
    ll a, b, c, d , p , k;
    sc("%lld %lld %lld %lld", &a, &b ,&c, &d);
///    if(a == 0 and b == 0 and c == 0 and)
    p = abs(a - c);
    k = abs(b - d);


         if(p == k)
            pf("Case %lld: 1\n",o);
            else if(p % 2 == 0 and k % 2 == 0)
                pf("Case %lld: 2\n",o);
                else if(p % 2 == 1 and k % 2 == 1)
                pf("Case %lld: 2\n",o);
            else
                pf("Case %lld: impossible\n",o);
  }
}
